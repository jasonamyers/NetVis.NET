@model IEnumerable<NetVis.Net.Models.Ip>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            Subnet
        </th>
        <th>
            Address
        </th>
        <th>
            Name
        </th>
        <th>
            Purpose
        </th>
        <th>
            Url
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Subnet.Network)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Address)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Purpose)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Url)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.IpId }) |
            @Html.ActionLink("Details", "Details", new { id=item.IpId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.IpId })
        </td>
    </tr>
}

</table>
